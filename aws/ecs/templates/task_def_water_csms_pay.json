[
    {
      "name": "csms-pay",
      "image": "${repository_url}",
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "${awslogs_group}",
          "awslogs-region": "${aws_region}",
          "awslogs-stream-prefix": "ecs"
        }
      },     
      "portMappings": [
        {
          "name": "csms-pay",
          "hostPort": 8081,
          "protocol": "tcp",
          "containerPort": 8081,
          "appProtocol": "http"
        }
      ],
      "environment": [
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "prod"
        }
      ]
    }
  ]